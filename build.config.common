BRANCH=android-5.4-stable
KMI_GENERATION=0
KERNEL_DIR=common

CC=clang
LD=ld.lld
NM=llvm-nm
OBJCOPY=llvm-objcopy
DEPMOD=depmod
CLANG_PREBUILT_BIN=prebuilts-master/clang/host/linux-x86/clang-dev/bin
BUILDTOOLS_PREBUILT_BIN=build/build-tools/path/linux-x86
CROSS_COMPILE_LD=ld
CROSS_COMPILE_OBJCOPY=objcopy

EXTRA_CMDS=''
STOP_SHIP_TRACEPRINTK=1
IN_KERNEL_MODULES=1
DO_NOT_STRIP_MODULES=1
